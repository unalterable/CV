\par \pard\plain \s2\ilvl1\outlinelevel1\sl240\slmult1\sb170\sa11\contextualspace\keepn\scaps0\caps0\cf0\expnd2\expndtw10\b\dbch\af8\dbch\af8\afs41\ab\loch\f5\fs16{\cs18\b\ab\afs16\rtlch \ltrch\loch\fs16\loch\f5
